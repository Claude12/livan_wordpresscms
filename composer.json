{
  "name": "livandigitalsolutions/livan-website",
  "description": "WordPress project with core installed in root using Composer",
  "type": "project",
  "require": {
    "johnpbloch/wordpress": "^6.8",
    "johnpbloch/wordpress-core-installer": "^2.0"
  },
  "extra": {
    "wordpress-install-dir": "./"
  },
  "config": {
    "preferred-install": "dist",
    "allow-plugins": {
      "johnpbloch/wordpress-core-installer": true
    }
  }
}
